{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.batluxebeauty.com"
        }
      ],
      "destination": "https://batluxebeauty.com/$1",
      "permanent": true
    }
  ]
}